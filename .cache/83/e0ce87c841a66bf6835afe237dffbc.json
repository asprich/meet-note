xt-editor v-model=\"model.notes\" label=\"Notes\"></rich-text-editor>\r\n            <text-box-list v-model=\"model.decisions\" label=\"Decisions\"></text-box-list>\r\n            <action-item-list v-model=\"model.actionItems\" label=\"Action Items\"></action-item-list>\r\n            <v-btn color=\"green\" dark @click=\"save\">Save</v-btn>\r\n        </v-form>\r\n        <v-snackbar v-model=\"successMessage\" top color=\"success\">\r\n            Your save was successful!\r\n            <v-btn dark text @click=\"successMessage = false\">Close</v-btn>\r\n        </v-snackbar>\r\n    </v-card>\r\n</template>\r\n\r\n<script>\r\nimport TextBoxList from \"../components/TextBoxList\";\r\nimport ActionItemList from \"../components/ActionItemList\";\r\nimport RichTextEditor from \"../components/RichTextEditor\";\r\nimport MeetingNoteFactory from \"../helpers/MeetingNoteFactory\";\r\nimport MeetingNoteRepository from \"../db/MeetingNoteRepository\";\r\nimport BetterCombobox from \"../components/BetterAutoCompleteComboBox\";\r\n\r\nexport default {\r\n    name: \"NoteEdit\",\r\n    components: { TextBoxList, ActionItemList, RichTextEditor, BetterCombobox},\r\n    data: () => ({\r\n        model: null,\r\n        attendeesInput: null,\r\n        tagsInput: null,\r\n        waiting: false,\r\n        successMessage: false\r\n    }),\r\n    computed: {\r\n        people() { return this.$store.getters.people },\r\n        tags() { return this.$store.getters.tags; }\r\n    },\r\n    methods: {\r\n        save() {\r\n            this.waiting = true;\r\n            this._meetingNoteRepository.save(this.model, () => {\r\n                this.waiting = false;\r\n                this.successMessage = true;\r\n            });\r\n        }\r\n    },\r\n    created() {\r\n        this._meetingNoteRepository = new MeetingNoteRepository();\r\n    },\r\n    mounted() {\r\n        this.model = MeetingNoteFactory.createMeetingNote();\r\n        \r\n        var meetingId = this.$route.params.meetingId;\r\n        if (meetingId)\r\n            this._meetingNoteRepository.get(meetingId, i => this.model = i);\r\n    }\r\n};\r\n</script>"},"lineCount":null}},"error":null,"hash":"77dfbb447d9ae5852eba6ebcb70362ef","cacheData":{"env":{}}}